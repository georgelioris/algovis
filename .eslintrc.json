{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "plugins": ["react-hooks", "react"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "react/jsx-indent-props": 0,
    "react/jsx-indent": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-props-no-spreading":0,
    "no-nested-ternary": "warn"
  }
}
